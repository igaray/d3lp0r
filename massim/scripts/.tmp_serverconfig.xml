<?xml version="1.0" encoding="UTF-8"?><!--DOCTYPE conf SYSTEM "helpers/2010/config.dtd"--><!-- launch-sync-type = timer or key--><conf backuppath="backup" launch-sync-type="key" reportpath="./backup/" time-to-launch="10000" tournamentmode="0" tournamentname="Mars2011">
    <simulation-server>
<network-agent backlog="10" port="12300"/>
</simulation-server>
    <match>
        <simulation configurationclass="massim.competition2011.GraphSimulationConfiguration" file-simulationlog="./backup/" id="mydemo" rmixmlobserver="massim.competition2011.GraphSimulationRMIXMLDocumentObserver" rmixmlobsserverhost="localhost" rmixmlobsserverport="1099" simulationclass="massim.competition2011.GraphSimulation">
            <configuration xmlns:meta="http://www.tu-clausthal.de/" agentsPerTeam="1" cellWidth="100" gridHeight="5" gridWidth="5" maxEdgeCost="10" maxNodeWeight="10" maxNumberOfSteps="10" minEdgeCost="1" minNodeWeight="1" numberOfAgents="2" numberOfNodes="20" numberOfTeams="2">
                <actions>
                    <action energyCost="2" energyCostDisabled="2" energyCostFailed="2" energyCostFailedDisabled="2" healthCost="0" healthCostDisabled="0" healthCostFailed="0" healthCostFailedDisabled="0" name="attack" pointsCost="0" pointsCostDisabled="0" pointsCostFailed="0" pointsCostFailedDisabled="0"/>
                    <action energyCost="2" energyCostDisabled="2" energyCostFailed="2" energyCostFailedDisabled="2" healthCost="0" healthCostDisabled="0" healthCostFailed="0" healthCostFailedDisabled="0" name="parry" pointsCost="0" pointsCostDisabled="0" pointsCostFailed="0" pointsCostFailedDisabled="0"/>
                    <action energyCost="1" energyCostDisabled="1" energyCostFailed="1" energyCostFailedDisabled="1" healthCost="0" healthCostDisabled="0" healthCostFailed="0" healthCostFailedDisabled="0" name="goto" pointsCost="0" pointsCostDisabled="0" pointsCostFailed="0" pointsCostFailedDisabled="0"/>
                    <action energyCost="1" energyCostDisabled="1" energyCostFailed="1" energyCostFailedDisabled="1" healthCost="0" healthCostDisabled="0" healthCostFailed="0" healthCostFailedDisabled="0" name="probe" pointsCost="0" pointsCostDisabled="0" pointsCostFailed="0" pointsCostFailedDisabled="0"/>
                    <action energyCost="1" energyCostDisabled="1" energyCostFailed="1" energyCostFailedDisabled="1" healthCost="0" healthCostDisabled="0" healthCostFailed="0" healthCostFailedDisabled="0" name="survey" pointsCost="0" pointsCostDisabled="0" pointsCostFailed="0" pointsCostFailedDisabled="0"/>
                    <action energyCost="2" energyCostDisabled="2" energyCostFailed="2" energyCostFailedDisabled="2" healthCost="0" healthCostDisabled="0" healthCostFailed="0" healthCostFailedDisabled="0" name="inspect" pointsCost="0" pointsCostDisabled="0" pointsCostFailed="0" pointsCostFailedDisabled="0"/>
                    <action energyCost="2" energyCostDisabled="3" energyCostFailed="2" energyCostFailedDisabled="3" healthCost="0" healthCostDisabled="0" healthCostFailed="0" healthCostFailedDisabled="0" name="repair" pointsCost="0" pointsCostDisabled="0" pointsCostFailed="0" pointsCostFailedDisabled="0"/>
                    <action energyCost="2" energyCostDisabled="2" energyCostFailed="2" energyCostFailedDisabled="2" healthCost="0" healthCostDisabled="0" healthCostFailed="0" healthCostFailedDisabled="0" name="buy" pointsCost="10" pointsCostDisabled="20" pointsCostFailed="0" pointsCostFailedDisabled="20"/>
                    <action energyCost="20" energyCostDisabled="10" energyCostFailed="0" energyCostFailedDisabled="0" healthCost="0" healthCostDisabled="0" healthCostFailed="0" healthCostFailedDisabled="0" name="recharge" pointsCost="0" pointsCostDisabled="0" pointsCostFailed="0" pointsCostFailedDisabled="0"/>
                </actions>
                <roles>
                    <role maxBuyEnergy="1000" maxBuyHealth="1000" maxBuyStrength="0" maxBuyVisRange="1000" maxEnergy="12" maxEnergyDisabled="12" maxHealth="4" name="Explorer" rateBuyEnergy="1" rateBuyEnergyDisabled="1" rateBuyHealth="1" rateBuyStrength="1" rateBuyVisRange="1" strength="0" visRange="2">
                        <actions>
                            <action name="goto"/>
                            <action name="probe"/>
                            <action name="survey"/>
                            <action name="buy"/>
                            <action name="recharge"/>
                        </actions>
                        <actionsDisable>
                            <action name="recharge"/>
                            <action name="goto"/>
                        </actionsDisable>
                    </role>
                    <role maxBuyEnergy="1000" maxBuyHealth="1000" maxBuyStrength="0" maxBuyVisRange="1000" maxEnergy="8" maxEnergyDisabled="8" maxHealth="6" name="Repairer" rateBuyEnergy="1" rateBuyEnergyDisabled="1" rateBuyHealth="1" rateBuyStrength="1" rateBuyVisRange="1" strength="0" visRange="1">
                        <actions>
                            <action name="goto"/>
                            <action name="parry"/>
                            <action name="survey"/>
                            <action name="buy"/>
                            <action name="repair"/>
                            <action name="recharge"/>
                        </actions>
                        <actionsDisable>
                            <action name="recharge"/>
                            <action name="goto"/>
                            <action name="repair"/>
                        </actionsDisable>
                    </role>
                    <role maxBuyEnergy="1000" maxBuyHealth="1000" maxBuyStrength="1000" maxBuyVisRange="1000" maxEnergy="7" maxEnergyDisabled="7" maxHealth="3" name="Saboteur" rateBuyEnergy="1" rateBuyEnergyDisabled="1" rateBuyHealth="1" rateBuyStrength="1" rateBuyVisRange="1" strength="3" visRange="1">
                        <actions>
                            <action name="goto"/>
                            <action name="parry"/>
                            <action name="survey"/>
                            <action name="buy"/>
                            <action name="attack"/>
                            <action name="recharge"/>
                        </actions>
                        <actionsDisable>
                            <action name="recharge"/>
                            <action name="goto"/>
                        </actionsDisable>
                    </role>
                    <role maxBuyEnergy="1000" maxBuyHealth="1000" maxBuyStrength="0" maxBuyVisRange="1000" maxEnergy="10" maxEnergyDisabled="10" maxHealth="1" name="Sentinel" rateBuyEnergy="1" rateBuyEnergyDisabled="1" rateBuyHealth="1" rateBuyStrength="1" rateBuyVisRange="1" strength="0" visRange="3">
                        <actions>
                            <action name="goto"/>
                            <action name="parry"/>
                            <action name="survey"/>
                            <action name="buy"/>
                            <action name="recharge"/>
                        </actions>
                        <actionsDisable>
                            <action name="recharge"/>
                            <action name="goto"/>
                        </actionsDisable>
                    </role>
                    <role maxBuyEnergy="1000" maxBuyHealth="1000" maxBuyStrength="0" maxBuyVisRange="1000" maxEnergy="8" maxEnergyDisabled="8" maxHealth="6" name="Inspector" rateBuyEnergy="1" rateBuyEnergyDisabled="1" rateBuyHealth="1" rateBuyStrength="1" rateBuyVisRange="1" strength="0" visRange="1">
                        <actions>
                            <action name="goto"/>
                            <action name="inspect"/>
                            <action name="survey"/>
                            <action name="buy"/>
                            <action name="recharge"/>
                        </actions>
                        <actionsDisable>
                            <action name="recharge"/>
                            <action name="goto"/>
                        </actionsDisable>
                    </role>
                </roles>
                <achievements>
