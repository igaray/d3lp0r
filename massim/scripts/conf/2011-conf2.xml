							<action name="skip"/>
							<action name="goto"/>
							<action name="attack"/>
							<action name="parry"/>
							<action name="probe"/>
							<action name="survey"/>
							<action name="inspect"/>
							<action name="repair"/>
							<action name="buy"/>
						</actions>
						<actionsDisable>
							<action name="skip"/>
						</actionsDisable>
					</role>
					<role name="disable"
							maxEnergy="5"  maxBuyEnergy="18"  rateBuyEnergy="2" maxEnergyDisabled="3" rateBuyEnergyDisabled="1"
							maxHealth="20" maxBuyHealth="35"  rateBuyHealth="2"
							strength="4"   maxBuyStrength="4" rateBuyStrength="0"
							visRange="2"   maxBuyVisRange="5" rateBuyVisRange="1">					
						<actions>
							<action name="skip"/>
						</actions>
						<actionsDisable>
							<action name="skip"/>
						</actionsDisable>
					</role>
				</roles>
