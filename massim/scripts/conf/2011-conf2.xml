<?xml version="1.0" encoding="UTF-8"?>
<!--DOCTYPE conf SYSTEM "helpers/2010/config.dtd"-->
<!-- launch-sync-type = timer or key-->
<conf   backuppath="backup"
		launch-sync-type="key"
		reportpath="./backup/"
		time-to-launch="10000"
		tournamentmode="0"
		tournamentname="GraphSimulation">
	<simulation-server><network-agent backlog="10" port="12300"/></simulation-server>
	<match>
		<simulation
				configurationclass="massim.competition2011.GraphSimulationConfiguration"
				file-simulationlog="./backup/"
				id="graph1"
				rmixmlobsserverhost="localhost"
				rmixmlobsserverport="1099"
				rmixmlobserver="massim.competition2011.GraphSimulationRMIXMLDocumentObserver"
				simulationclass="massim.competition2011.GraphSimulation"
				
		>
			<configuration
					xmlns:meta="http://www.tu-clausthal.de/"
					maxNumberOfSteps="5"
					numberOfAgents="20"
					numberOfTeams="2"
					agentsPerTeam="1"
					numberOfNodes="20"
					gridWidth="5"
					gridHeight="5"
					cellWidth="100"
					minNodeWeight="1"
					maxNodeWeight="10"
					minEdgeCost="1"
					maxEdgeCost="10"
			>
				<actions>
					<action
						name="attack"
						energyCost="1" energyCostFailed="1" energyCostDisabled="2" energyCostFailedDisabled="2"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="parry"
						energyCost="1" energyCostFailed="1" energyCostDisabled="2" energyCostFailedDisabled="2"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="goto"
						energyCost="1" energyCostFailed="1" energyCostDisabled="2" energyCostFailedDisabled="2"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="probe"
						energyCost="1" energyCostFailed="1" energyCostDisabled="2" energyCostFailedDisabled="2"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="survey"
						energyCost="1" energyCostFailed="1" energyCostDisabled="2" energyCostFailedDisabled="2"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="inspect"
						energyCost="1" energyCostFailed="1" energyCostDisabled="2" energyCostFailedDisabled="2"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="repair"
						energyCost="2" energyCostFailed="1" energyCostDisabled="4" energyCostFailedDisabled="4"
						healthCost="0" healthCostFailed="0" healthCostDisabled="0" healthCostFailedDisabled="0"
						pointsCost="0" pointsCostFailed="0" pointsCostDisabled="0" pointsCostFailedDisabled="0"
					/>
					<action
						name="buy"
						energyCost="1"  energyCostFailed="1" energyCostDisabled="1"  energyCostFailedDisabled="1"
						healthCost="0"  healthCostFailed="0" healthCostDisabled="0"  healthCostFailedDisabled="0"
						pointsCost="10" pointsCostFailed="0" pointsCostDisabled="20" pointsCostFailedDisabled="20"
					/>
					<action
						name="skip"
						energyCost="20" energyCostFailed="0"  energyCostDisabled="20" energyCostFailedDisabled="0"
						healthCost="0"  healthCostFailed="0"  healthCostDisabled="0"  healthCostFailedDisabled="0"
						pointsCost="0"  pointsCostFailed="0"  pointsCostDisabled="0"  pointsCostFailedDisabled="0"
					/>
				</actions>
				<roles>
					<role name="master"
							maxEnergy="9" maxBuyEnergy="20"  rateBuyEnergy="2" maxEnergyDisabled="4" rateBuyEnergyDisabled="1"
							maxHealth="9" maxBuyHealth="15"  rateBuyHealth="2"
							strength="5" maxBuyStrength="5"  rateBuyStrength="0"
							visRange="2"  maxBuyVisRange="5" rateBuyVisRange="1">
						<actions>
							<action name="skip"/>
							<action name="goto"/>
							<action name="attack"/>
							<action name="parry"/>
							<action name="probe"/>
							<action name="survey"/>
							<action name="inspect"/>
							<action name="repair"/>
							<action name="buy"/>
						</actions>
						<actionsDisable>
							<action name="skip"/>
							<action name="goto"/>
							<action name="attack"/>
							<action name="parry"/>
							<action name="probe"/>
							<action name="survey"/>
							<action name="inspect"/>
							<action name="repair"/>
